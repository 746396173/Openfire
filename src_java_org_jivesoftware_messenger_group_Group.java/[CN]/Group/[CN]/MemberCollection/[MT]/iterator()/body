{
  return new Iterator(){
    Iterator iter=users.iterator();
    Object current=null;
    public boolean hasNext(){
      return iter.hasNext();
    }
    public Object next(){
      current=iter.next();
      return current;
    }
    public void remove(){
      if (current == null) {
        throw new IllegalStateException();
      }
      provider.deleteMember(name,(String)current);
      iter.remove();
    }
  }
;
}
