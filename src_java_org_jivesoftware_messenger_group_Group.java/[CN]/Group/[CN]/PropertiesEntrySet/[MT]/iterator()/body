{
  return new Iterator(){
    Iterator iter=properties.entrySet().iterator();
    Map.Entry current=null;
    public boolean hasNext(){
      return iter.hasNext();
    }
    public Object next(){
      current=(Map.Entry)iter.next();
      return iter.next();
    }
    public void remove(){
      if (current == null) {
        throw new IllegalStateException();
      }
      deleteProperty((String)current.getKey());
      iter.remove();
    }
  }
;
}
