{
  if (is == null) {
    return null;
  }
  try {
    ByteArrayOutputStream swapStream=new ByteArrayOutputStream();
    byte[] buff=new byte[100];
    int rc=0;
    while ((rc=is.read(buff,0,100)) > 0) {
      swapStream.write(buff,0,rc);
    }
    byte[] result=swapStream.toByteArray();
    return result;
  }
 catch (  IOException e) {
    throw e;
  }
 finally {
    is.close();
  }
}
