{
  PrivacyList privacyList=null;
  java.sql.Connection con=null;
  PreparedStatement pstmt=null;
  SAXReader xmlReader=null;
  try {
    xmlReader=xmlReaders.take();
    con=DbConnectionManager.getConnection();
    pstmt=con.prepareStatement(LOAD_PRIVACY_LIST);
    pstmt.setString(1,username);
    pstmt.setString(2,listName);
    ResultSet rs=pstmt.executeQuery();
    while (rs.next()) {
      boolean isDefault=rs.getInt(1) == 1;
      Element listElement=xmlReader.read(new StringReader(rs.getString(2))).getRootElement();
      privacyList=new PrivacyList(username,listName,isDefault,listElement);
    }
  }
 catch (  Exception e) {
    Log.error("Error loading privacy list: " + listName + " of username: "+ username,e);
  }
 finally {
    if (xmlReader != null) {
      xmlReaders.add(xmlReader);
    }
    try {
      if (pstmt != null) {
        pstmt.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
    try {
      if (con != null) {
        con.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
  }
  return privacyList;
}
