{
  if (this.isElement(id))   return this;
  if (children != null) {
    for (int i=0; i < children.size(); i++) {
      ElementType entry=(ElementType)children.get(i);
      if (entry.isElement(id))       return entry;
      entry=entry.findElement(id);
      if (entry != null)       return entry;
    }
  }
  return null;
}
