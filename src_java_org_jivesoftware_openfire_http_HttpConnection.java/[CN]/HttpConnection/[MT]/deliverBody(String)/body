{
synchronized (this) {
    if (isClosed) {
      throw new HttpConnectionClosedException("The http connection is no longer " + "available to deliver content");
    }
    isClosed=true;
  }
  if (body == null) {
    body=HttpBindServlet.createEmptyBody(false);
  }
  HttpBindServlet.respond(this.getSession(),this.context,body);
}
