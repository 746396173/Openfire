{
synchronized (this) {
    if (isClosed) {
      return;
    }
  }
  try {
    deliverBody(null);
  }
 catch (  HttpConnectionClosedException e) {
    Log.warn("Unexpected exception occurred while trying to close an HttpException.",e);
  }
catch (  IOException e) {
    Log.warn("Unexpected exception occurred while trying to close an HttpException.",e);
  }
}
