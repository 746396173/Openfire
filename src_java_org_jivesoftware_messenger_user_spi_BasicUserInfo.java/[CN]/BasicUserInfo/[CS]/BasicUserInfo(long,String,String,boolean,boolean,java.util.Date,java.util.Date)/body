{
  this.id=id;
  this.creationDate=creationDate;
  this.modificationDate=modificationDate;
  if (email == null || "".equals(email)) {
    this.email=" ";
  }
 else {
    this.email=email;
  }
  if (name != null) {
    this.name=name;
  }
  this.nameVisible=nameVisible;
  this.emailVisible=emailVisible;
}
