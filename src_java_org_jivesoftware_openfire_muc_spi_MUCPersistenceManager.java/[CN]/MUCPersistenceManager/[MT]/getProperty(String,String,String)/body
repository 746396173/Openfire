{
  MUCServiceProperties properties=propertyMaps.get(subdomain);
  if (properties == null) {
    properties=new MUCServiceProperties(subdomain);
    propertyMaps.put(subdomain,properties);
  }
  String value=properties.get(name);
  if (value != null) {
    return value;
  }
 else {
    return defaultValue;
  }
}
