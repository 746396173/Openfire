{
  if (registrationID == null || username == null) {
    throw new NullPointerException("Arguments cannot be null.");
  }
  this.registrationID=registrationID;
  this.username=username;
  this.nickname=nickname;
  this.groups=groups;
  try {
    insertIntoDb();
  }
 catch (  Exception e) {
    Log.error(e);
  }
}
