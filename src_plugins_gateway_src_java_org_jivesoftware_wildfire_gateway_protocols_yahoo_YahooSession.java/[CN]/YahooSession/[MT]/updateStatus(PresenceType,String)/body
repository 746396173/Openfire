{
  try {
    if (isLoggedIn()) {
      yahooSession.setStatus(((YahooTransport)getTransport()).convertJabStatusToYahoo(presenceType));
    }
 else {
    }
  }
 catch (  Exception e) {
    Log.error("Unable to set Yahoo Status:",e);
  }
  this.presenceType=presenceType;
  this.verboseStatus=verboseStatus;
}
