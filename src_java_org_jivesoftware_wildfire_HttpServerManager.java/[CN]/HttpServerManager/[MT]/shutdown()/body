{
  if (httpBindServer != null) {
    try {
      httpBindServer.stop();
    }
 catch (    Exception e) {
      Log.error("Error stopping http bind server",e);
    }
    httpBindServer=null;
  }
  if (adminServer != null && adminServer != httpBindServer) {
    try {
      adminServer.stop();
    }
 catch (    Exception e) {
      Log.error("Error stopping admin console server",e);
    }
    adminServer=null;
  }
}
