{
  boolean trusted=true;
  try {
    x509Certificates[0].checkValidity();
  }
 catch (  CertificateExpiredException e) {
    Log.error(LocaleUtils.getLocalizedString("admin.error"),e);
    trusted=false;
  }
catch (  CertificateNotYetValidException e) {
    Log.error(LocaleUtils.getLocalizedString("admin.error"),e);
    trusted=false;
  }
  return trusted;
}
