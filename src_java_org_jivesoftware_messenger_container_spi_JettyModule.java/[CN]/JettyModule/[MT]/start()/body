{
  try {
    jetty.start();
    ServiceItem serverItem=new ServiceItem(null,this,null);
    reg=serverContainer.getServiceLookup().register(serverItem);
    Log.info("Started embedded web server on port: " + port);
  }
 catch (  Exception e) {
    Log.error("Trouble starting Jetty",e);
    stop();
  }
}
