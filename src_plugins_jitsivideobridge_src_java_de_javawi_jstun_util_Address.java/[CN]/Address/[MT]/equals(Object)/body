{
  if (obj == null)   return false;
  try {
    byte[] data1=this.getBytes();
    byte[] data2=((Address)obj).getBytes();
    if ((data1[0] == data2[0]) && (data1[1] == data2[1]) && (data1[2] == data2[2])&& (data1[3] == data2[3]))     return true;
    return false;
  }
 catch (  UtilityException ue) {
    return false;
  }
}
