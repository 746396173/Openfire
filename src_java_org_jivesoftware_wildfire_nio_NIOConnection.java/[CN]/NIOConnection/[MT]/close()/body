{
  boolean wasClosed=false;
synchronized (this) {
    if (!isClosed()) {
      try {
        deliverRawText(flashClient ? "</flash:stream>" : "</stream:stream>",false);
      }
 catch (      Exception e) {
      }
      closeConnection();
      wasClosed=true;
    }
  }
  if (wasClosed) {
    notifyCloseListeners();
  }
}
