{
  Logger.writeFile("Call " + memberReceiver.toString() + ":  "+ "Dtmf detector calls:  "+ numberOfTimesCalled);
  if (numberOfTimesCalled != 0) {
    Logger.writeFile(memberReceiver.toString() + ":  Dtmf decoder average ms per call:  " + ((float)((float)totalDecodeTime / numberOfTimesCalled)));
  }
}
