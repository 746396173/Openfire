{
  Participant principal=findParticipantForMucAddress(fromJid);
  if (principal == null) {
    logger.error("Failed to perform mute operation - " + fromJid + " not exists in the conference.");
    return false;
  }
  if (!fromJid.equals(toBeMutedJid) && ChatRoomMemberRole.MODERATOR.compareTo(principal.getChatMember().getRole()) < 0) {
    logger.error("Permission denied for mute operation from " + fromJid);
    return false;
  }
  Participant participant=findParticipantForMucAddress(toBeMutedJid);
  if (participant == null) {
    logger.error("Participant for jid: " + toBeMutedJid + " not found");
    return false;
  }
  logger.info("Will " + (doMute ? "mute" : "unmute") + " "+ toBeMutedJid+ " on behalf of "+ fromJid);
  boolean succeeded=colibri.muteParticipant(participant.getColibriChannelsInfo(),doMute);
  if (succeeded) {
    participant.setMuted(doMute);
  }
  return succeeded;
}
