{
  try {
    if (add) {
      for (int i=0; i < inData.length; i++) {
        mixData[i]=mixData[i] + inData[i];
      }
    }
 else {
      for (int i=0; i < inData.length; i++) {
        mixData[i]=mixData[i] - inData[i];
      }
    }
  }
 catch (  IndexOutOfBoundsException e) {
    Logger.println("Exception!  inData length " + inData.length + " mixData length "+ mixData.length+ " add "+ add);
    e.printStackTrace();
  }
}
