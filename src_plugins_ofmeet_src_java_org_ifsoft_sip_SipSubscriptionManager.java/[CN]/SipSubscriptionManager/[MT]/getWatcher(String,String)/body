{
synchronized (watchers) {
    List<SipSubscription> subs=watchers.get(user);
    if (subs != null) {
      for (      SipSubscription sub : subs) {
        String subDest=((SipURI)sub.remoteParty.getURI()).getUser();
        if (subDest.equals(dest)) {
          return sub;
        }
      }
    }
  }
  return null;
}
