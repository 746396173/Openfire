{
synchronized (subscriptions) {
    List<SipSubscription> subs=subscriptions.get(user);
    if (subs != null) {
      for (      SipSubscription sub : subs) {
        String subDest=((SipURI)sub.remoteParty.getURI()).getUser();
        if (subDest.equals(dest)) {
          sub.cancel();
          subs.remove(sub);
          deleteSubscription(sub);
          return sub;
        }
      }
    }
  }
  return null;
}
