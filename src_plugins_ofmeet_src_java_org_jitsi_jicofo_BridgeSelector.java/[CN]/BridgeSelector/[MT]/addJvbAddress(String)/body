{
  String pubSubNode=findNodeForBridge(bridgeJid);
  if (pubSubNode != null) {
    logger.info("Subscribing to pubsub notfications to " + pubSubNode + " for "+ bridgeJid);
    subscriptionOpSet.subscribe(pubSubNode,this);
  }
 else {
    logger.warn("No pub-sub node mapped for " + bridgeJid + " statistics will not be tracked for this instance.");
  }
  bridges.put(bridgeJid,new BridgeState(bridgeJid));
}
