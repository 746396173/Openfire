{
  final String body="<div class=\"jive-contentBoxHeader\">" + title + "</div>\n"+ "<div class=\"jive-contentBox\">"+ bodyContent.getString()+ "</div>\n";
  try {
    pageContext.getOut().write(body);
  }
 catch (  IOException ioe) {
    throw new JspException(ioe.getMessage());
  }
  return super.doEndTag();
}
