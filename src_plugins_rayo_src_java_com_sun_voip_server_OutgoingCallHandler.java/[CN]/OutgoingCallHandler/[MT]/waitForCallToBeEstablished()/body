{
  if (cp.getProtocol().equalsIgnoreCase("WebRtc")) {
    return true;
  }
synchronized (waitCallEstablishedLock) {
    if (done || reasonCallEnded != null) {
      return false;
    }
    try {
      waitCallEstablishedLock.wait();
    }
