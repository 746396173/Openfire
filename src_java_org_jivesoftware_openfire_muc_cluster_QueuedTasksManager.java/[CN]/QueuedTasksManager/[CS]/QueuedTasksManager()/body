{
  TaskEngine.getInstance().scheduleAtFixedRate(new TimerTask(){
    public void run(){
      if (!ClusterManager.isClusteringStarting()) {
        MUCRoomTask mucRoomTask;
        while ((mucRoomTask=taskQueue.poll()) != null) {
          mucRoomTask.run();
        }
      }
    }
  }
,1000,30000);
}
