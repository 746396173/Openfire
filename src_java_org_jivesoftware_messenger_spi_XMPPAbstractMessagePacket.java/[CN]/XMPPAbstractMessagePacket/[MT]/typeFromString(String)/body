{
  if (CHAT.toString().equals(type)) {
    return CHAT;
  }
 else   if (GROUP_CHAT.toString().equals(type)) {
    return GROUP_CHAT;
  }
 else   if (HEADLINE.toString().equals(type)) {
    return HEADLINE;
  }
 else   if (ERROR.toString().equals(type)) {
    return ERROR;
  }
 else {
    return NORMAL;
  }
}
