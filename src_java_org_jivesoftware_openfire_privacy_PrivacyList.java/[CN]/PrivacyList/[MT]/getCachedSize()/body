{
  int size=0;
  size+=CacheSizes.sizeOfObject();
  size+=CacheSizes.sizeOfString(userJID.toString());
  size+=CacheSizes.sizeOfString(name);
  size+=CacheSizes.sizeOfBoolean();
  size+=CacheSizes.sizeOfCollection(items);
  if (roster != null) {
    size+=roster.getCachedSize();
  }
  return size;
}
