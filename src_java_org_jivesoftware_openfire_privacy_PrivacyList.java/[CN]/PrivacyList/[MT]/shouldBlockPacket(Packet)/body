{
  if (packet.getFrom() == null) {
    return false;
  }
  Roster roster=getRoster();
  for (  PrivacyItem item : items) {
    if (item.matchesCondition(packet,roster,userJID)) {
      if (item.isAllow()) {
        return false;
      }
      if (Log.isDebugEnabled()) {
        Log.debug("Packet was blocked: " + packet);
      }
      return true;
    }
  }
  return false;
}
