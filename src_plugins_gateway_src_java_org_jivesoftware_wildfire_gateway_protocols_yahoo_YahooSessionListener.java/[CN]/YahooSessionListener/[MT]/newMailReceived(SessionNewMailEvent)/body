{
  if (event.getMailCount() > 0) {
    yahooSession.getTransport().sendMessage(yahooSession.getJIDWithHighestPriority(),yahooSession.getTransport().getJID(),LocaleUtils.getLocalizedString("gateway.yahoo.mail","gateway",Arrays.asList(Integer.toString(event.getMailCount()))),Message.Type.headline);
  }
}
