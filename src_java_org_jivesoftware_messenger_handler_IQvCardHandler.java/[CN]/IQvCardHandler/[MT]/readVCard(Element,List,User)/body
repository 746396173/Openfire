{
  Iterator children=element.elementIterator();
  while (children.hasNext()) {
    Element child=(Element)children.next();
    nameStack.add(child.getName());
    String value=child.getTextTrim();
    if (value != null) {
      if (!"".equals(value)) {
        VCardManager.getInstance().setVCardProperty(user.getUsername(),createName(nameStack),value);
      }
 else {
        VCardManager.getInstance().deleteVCardProperty(user.getUsername(),createName(nameStack));
      }
    }
    readVCard(child,nameStack,user);
    nameStack.remove(nameStack.size() - 1);
  }
}
