{
  for (  Integer i : resources.keySet()) {
    if (resources.get(i).equals(resource)) {
      resources.remove(i);
      try {
        getTransport().notifyRosterOffline(new JID(getJID().getNode(),getJID().getDomain(),resource));
      }
 catch (      UserNotFoundException e) {
      }
      break;
    }
  }
}
