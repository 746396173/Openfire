{
  this.element=element;
  String to=element.attributeValue("to");
  if (to != null) {
    toJID=new JID(to);
    element.addAttribute("to",toJID.toString());
  }
  String from=element.attributeValue("from");
  if (from != null) {
    fromJID=new JID(from);
    element.addAttribute("from",fromJID.toString());
  }
}
