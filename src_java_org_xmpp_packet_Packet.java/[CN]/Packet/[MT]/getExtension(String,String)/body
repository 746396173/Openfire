{
  List extensions=element.elements(QName.get(name,namespace));
  if (!extensions.isEmpty()) {
    Class extensionClass=PacketExtension.getExtensionClass(name,namespace);
    if (extensionClass != null) {
      try {
        Constructor constructor=extensionClass.getDeclaredConstructor(new Class[]{Element.class});
        return (PacketExtension)constructor.newInstance(new Object[]{extensions.get(0)});
      }
 catch (      Exception e) {
      }
    }
  }
  return null;
}
