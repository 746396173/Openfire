{
  String from=element.attributeValue("from");
  if (from == null || from.length() == 0) {
    return null;
  }
 else {
    if (fromJID != null && from.equals(fromJID.toString())) {
      return fromJID;
    }
 else {
      String[] parts=JID.getParts(from);
      fromJID=new JID(parts[0],parts[1],parts[2],true);
      return fromJID;
    }
  }
}
