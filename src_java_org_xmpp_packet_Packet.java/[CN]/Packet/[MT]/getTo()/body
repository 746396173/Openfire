{
  String to=element.attributeValue("to");
  if (to == null) {
    return null;
  }
 else {
    if (toJID != null && to.equals(toJID.toString())) {
      return toJID;
    }
 else {
      String[] parts=JID.getParts(to);
      toJID=new JID(parts[0],parts[1],parts[2],true);
      return toJID;
    }
  }
}
