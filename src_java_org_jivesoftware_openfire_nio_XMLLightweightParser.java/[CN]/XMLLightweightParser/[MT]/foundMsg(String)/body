{
  if (msg != null) {
    if (hasIllegalCharacterReferences(msg)) {
      throw new Exception("Illegal character reference found in: " + msg);
    }
    msgs.add(msg);
  }
  status=XMLLightweightParser.INIT;
  tailCount=0;
  cdataOffset=0;
  head.setLength(0);
  insideRootTag=false;
  insideChildrenTag=false;
  depth=0;
}
