{
  Component component=getComponent(packet.getFrom().getDomain());
  if (component != null) {
    if (packet instanceof IQ && IQ.Type.result == ((IQ)packet).getType()) {
      IQ iq=(IQ)packet;
      Element childElement=iq.getChildElement();
      String namespace=null;
      if (childElement != null) {
        namespace=childElement.getNamespaceURI();
      }
      if ("http://jabber.org/protocol/disco#info".equals(namespace)) {
        XMPPServer.getInstance().getIQDiscoItemsHandler().addComponentItem(packet.getFrom().toBareJID(),childElement.element("identity").attributeValue("name"));
      }
    }
  }
}
