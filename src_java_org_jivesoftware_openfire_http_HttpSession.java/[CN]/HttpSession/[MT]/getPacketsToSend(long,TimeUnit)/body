{
  if (isClosed) {
    throw new HttpConnectionClosedException("the connection has been closed");
  }
  try {
    if (!packetsToSendSemaphore.tryAcquire(time,timeUnit) || packetsToSend.size() <= 0) {
      return null;
    }
  }
 catch (  InterruptedException e) {
    return null;
  }
  return packetsToSend.remove();
}
