{
  File wildfireHome=new File(homeGuess);
  File configFile=new File(wildfireHome,jiveConfigName);
  if (!configFile.exists()) {
    throw new FileNotFoundException();
  }
 else {
    try {
      return new File(wildfireHome.getCanonicalPath());
    }
 catch (    Exception ex) {
      throw new FileNotFoundException();
    }
  }
}
