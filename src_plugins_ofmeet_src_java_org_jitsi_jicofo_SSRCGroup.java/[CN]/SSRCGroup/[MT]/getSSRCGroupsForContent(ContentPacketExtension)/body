{
  List<SSRCGroup> groups=new ArrayList<SSRCGroup>();
  RtpDescriptionPacketExtension rtpDescPe=JingleUtils.getRtpDescription(content);
  if (rtpDescPe == null) {
    return groups;
  }
  List<SourceGroupPacketExtension> groupExtensions=rtpDescPe.getChildExtensionsOfType(SourceGroupPacketExtension.class);
  for (  SourceGroupPacketExtension groupPe : groupExtensions) {
    groups.add(new SSRCGroup(groupPe));
  }
  return groups;
}
