{
  String sql="SELECT * FROM sipPhoneLog";
  sql=SQLCondition != null && !SQLCondition.equals("") ? sql + " WHERE " + SQLCondition : sql;
  sql+=" ORDER BY datetime DESC";
  List<CallLog> calls=new ArrayList<CallLog>(numResults);
  Connection con=null;
  PreparedStatement pstmt=null;
  try {
    con=DbConnectionManager.getConnection();
    pstmt=DbConnectionManager.createScrollablePreparedStatement(con,sql);
    ResultSet rs=pstmt.executeQuery();
    DbConnectionManager.setFetchSize(rs,startIndex + numResults);
    DbConnectionManager.scrollResultSet(rs,startIndex);
    int count=0;
    while (rs.next() && count < numResults) {
      calls.add(read(rs));
      count++;
    }
    rs.close();
  }
 catch (  SQLException e) {
    Log.error(e);
  }
 finally {
    try {
      if (pstmt != null) {
        pstmt.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
    try {
      if (con != null) {
        con.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
  }
  return calls;
}
