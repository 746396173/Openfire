{
  PropertyEventDispatcher.removeListener(this);
  try {
    componentManager.removeComponent(serviceName);
  }
 catch (  Exception e) {
    componentManager.getLog().error(e);
  }
  try {
    componentManager.removeComponent(LogComponent.NAME);
  }
 catch (  Exception e) {
    componentManager.getLog().error(e);
  }
  sipComponent=null;
  logComponent=null;
  componentManager=null;
}
