{
  if (!running) {
    return;
  }
  Log.info("Stopping transport service: " + type.toString());
  PropertyEventDispatcher.removeListener(this);
  try {
    componentManager.removeComponent(this.subDomain);
  }
 catch (  Exception e) {
    componentManager.getLog().error(e);
  }
  transport=null;
  running=false;
}
