{
  Connection con=null;
  PreparedStatement pstmt=null;
  try {
    con=DbConnectionManager.getConnection();
    pstmt=con.prepareStatement(MATCH_AUTHORIZED);
    pstmt.setString(1,username);
    pstmt.setString(2,principal);
    ResultSet rs=pstmt.executeQuery();
    return rs.next();
  }
 catch (  Exception e) {
    return false;
  }
 finally {
    try {
      if (pstmt != null) {
        pstmt.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
    try {
      if (con != null) {
        con.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
  }
}
