{
  boolean needsReload=false;
  if (this.acceptSelfSigned != acceptSelfSigned) {
    this.acceptSelfSigned=acceptSelfSigned;
    needsReload=true;
  }
  if (this.checkValidity != checkValidity) {
    this.checkValidity=checkValidity;
    needsReload=true;
  }
  if (needsReload) {
    reload();
  }
}
