{
  try {
    sockets.remove(digest);
    if (xmppSession != null)     xmppSession.close();
    xmppSession=null;
  }
 catch (  Exception e) {
    Log.error("An error occurred while attempting to remove the socket and xmppSession",e);
  }
  Log.debug(digest + " : onClose : " + statusCode+ " "+ reason);
}
