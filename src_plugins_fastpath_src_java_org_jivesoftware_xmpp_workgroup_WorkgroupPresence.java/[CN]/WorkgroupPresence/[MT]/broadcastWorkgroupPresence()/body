{
  TaskEngine.getInstance().submit(new Runnable(){
    public void run(){
      try {
        for (        String bareJID : presenceSubscribers) {
          sendPresence(new JID(bareJID));
        }
        for (        JID tempSubscriber : listeners) {
          sendPresence(tempSubscriber);
        }
      }
 catch (      Exception e) {
        ComponentManagerFactory.getComponentManager().getLog().error("Error broadcasting available presence",e);
      }
    }
  }
);
}
