{
  Connection con=null;
  PreparedStatement pstmt=null;
  ResultSet rs=null;
  try {
    con=DbConnectionManager.getConnection();
    pstmt=con.prepareStatement(LOAD_ROSTER);
    pstmt.setLong(1,workgroup.getID());
    rs=pstmt.executeQuery();
    List<String> jids=new ArrayList<String>();
    while (rs.next()) {
      jids.add(rs.getString(1));
    }
    presenceSubscribers.addAll(jids);
  }
 catch (  SQLException e) {
    ComponentManagerFactory.getComponentManager().getLog().error("Error loading workgroup roster items ",e);
  }
 finally {
    DbConnectionManager.closeConnection(rs,pstmt,con);
  }
}
