{
  if (startIndex == 0) {
    return 0;
  }
  if (keys[startIndex] == value) {
    return startIndex;
  }
  char siblingIndex=leftChildren[startIndex];
  while (siblingIndex != 0) {
    char recursiveIndex=findKey(value,siblingIndex);
    if (recursiveIndex != 0) {
      return recursiveIndex;
    }
 else {
      siblingIndex=rightSiblings[siblingIndex];
    }
  }
  return 0;
}
