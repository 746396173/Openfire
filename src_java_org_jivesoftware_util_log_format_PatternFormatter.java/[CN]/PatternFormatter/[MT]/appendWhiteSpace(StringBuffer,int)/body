{
  while (length >= 16) {
    sb.append(SPACE_16);
    length-=16;
  }
  if (length >= 8) {
    sb.append(SPACE_8);
    length-=8;
  }
  if (length >= 4) {
    sb.append(SPACE_4);
    length-=4;
  }
  if (length >= 2) {
    sb.append(SPACE_2);
    length-=2;
  }
  if (length >= 1) {
    sb.append(SPACE_1);
    length-=1;
  }
}
