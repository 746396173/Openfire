{
  boolean result=false;
  if (groupsInList != null) {
    Group group=Group.resolveFrom(item);
    if (group != null) {
      result=true;
      if (addOrRemove == ADD) {
        groupsInList.add(group);
      }
 else       if (addOrRemove == REMOVE) {
        groupsInList.remove(group);
      }
    }
  }
  return result;
}
