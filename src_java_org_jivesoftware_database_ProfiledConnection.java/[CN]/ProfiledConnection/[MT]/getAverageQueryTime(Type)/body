{
  long time, count;
switch (type) {
case select:
    count=selectCount;
  time=totalSelectTime;
break;
case update:
count=updateCount;
time=totalUpdateTime;
break;
case insert:
count=insertCount;
time=totalInsertTime;
break;
case delete:
count=deleteCount;
time=totalDeleteTime;
break;
default :
throw new IllegalArgumentException("Invalid type");
}
if (count != 0) {
return time / (double)count;
}
 else {
return 0.0;
}
}
