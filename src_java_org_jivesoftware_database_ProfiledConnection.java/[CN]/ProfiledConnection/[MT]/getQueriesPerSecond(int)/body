{
  long count, start, end;
switch (type) {
case SELECT:
    count=selectCount;
  start=startSelectTime;
end=endSelectTime;
break;
case UPDATE:
count=updateCount;
start=startUpdateTime;
end=endUpdateTime;
break;
case INSERT:
count=insertCount;
start=startInsertTime;
end=endInsertTime;
break;
case DELETE:
count=deleteCount;
start=startDeleteTime;
end=endDeleteTime;
break;
default :
throw new IllegalArgumentException("Invalid type");
}
if (count == 0) {
return 0;
}
if (end == 0) {
end=System.currentTimeMillis();
}
double time=(end - start) / 1000.0;
return count / time;
}
