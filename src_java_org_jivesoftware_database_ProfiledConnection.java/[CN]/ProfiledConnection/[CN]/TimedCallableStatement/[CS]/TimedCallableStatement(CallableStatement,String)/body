{
  this.cstmt=cstmt;
  this.sql=sql;
  String sqlL=sql.toLowerCase().trim();
  if (sqlL.startsWith("insert")) {
    type=Type.insert;
  }
 else   if (sqlL.startsWith("update")) {
    type=Type.update;
  }
 else   if (sqlL.startsWith("delete")) {
    type=Type.delete;
  }
 else {
    type=Type.select;
  }
}
