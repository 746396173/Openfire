{
  long t1=System.currentTimeMillis();
  int result=pstmt.executeUpdate();
  long t2=System.currentTimeMillis();
switch (type) {
case SELECT:
    addQuery(SELECT,sql,t2 - t1);
  break;
case UPDATE:
addQuery(UPDATE,sql,t2 - t1);
break;
case INSERT:
addQuery(INSERT,sql,t2 - t1);
break;
case DELETE:
addQuery(DELETE,sql,t2 - t1);
break;
}
return result;
}
