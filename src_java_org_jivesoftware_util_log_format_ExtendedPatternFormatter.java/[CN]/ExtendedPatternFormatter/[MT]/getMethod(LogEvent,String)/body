{
  final ContextMap map=event.getContextMap();
  if (null != map) {
    final Object object=map.get("method");
    if (null != object) {
      return object.toString();
    }
  }
  final String result=StackIntrospector.getCallerMethod(Log.class);
  if (null == result) {
    return "UnknownMethod";
  }
  return result;
}
