{
  if (date == null) {
    return null;
  }
  int index=date.lastIndexOf(":");
  date=date.substring(0,index) + date.substring(index + 1);
  Date d=null;
  try {
    if (date.length() == 24) {
      d=dateFormatNoMil.parse(date);
    }
 else {
      d=dateFormatMil.parse(date);
    }
  }
 catch (  ParseException e) {
  }
  return d;
}
