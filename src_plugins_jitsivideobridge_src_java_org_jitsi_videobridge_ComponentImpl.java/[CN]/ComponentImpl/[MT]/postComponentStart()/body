{
  super.postComponentStart();
  String trueString=Boolean.toString(true);
  System.setProperty(ConfigurationService.PNAME_CONFIGURATION_FILE_IS_READ_ONLY,trueString);
  System.setProperty(MediaServiceImpl.DISABLE_AUDIO_SUPPORT_PNAME,trueString);
  System.setProperty(MediaServiceImpl.DISABLE_VIDEO_SUPPORT_PNAME,trueString);
  if (System.getProperty(DeviceConfiguration.PROP_VIDEO_RTP_PACING_THRESHOLD) == null) {
    System.setProperty(DeviceConfiguration.PROP_VIDEO_RTP_PACING_THRESHOLD,Integer.toString(Integer.MAX_VALUE));
  }
  startOSGi();
}
