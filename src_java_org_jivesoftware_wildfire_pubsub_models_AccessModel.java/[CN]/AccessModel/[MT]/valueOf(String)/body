{
  if ("open".equals(name)) {
    return open;
  }
 else   if ("whitelist".equals(name)) {
    return whitelist;
  }
 else   if ("authorize".equals(name)) {
    return authorize;
  }
 else   if ("presence".equals(name)) {
    return presence;
  }
 else   if ("roster".equals(name)) {
    return roster;
  }
  throw new IllegalArgumentException("Unknown access model: " + name);
}
