{
  Connection con=null;
  try {
    con=getConnection();
    Statement stmt=con.createStatement();
    stmt.execute("SHUTDOWN");
    stmt.close();
  }
 catch (  SQLException sqle) {
    Log.error(sqle);
  }
 finally {
    try {
      if (con != null) {
        con.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
  }
  settings=null;
}
