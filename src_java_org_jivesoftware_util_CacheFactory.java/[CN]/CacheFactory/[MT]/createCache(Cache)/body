{
  String name=cache.getName();
  int maxCacheSize=cache.getMaxCacheSize();
  long maxLifetime=cache.getMaxLifetime();
  Cache newCache=(Cache)caches.get(name);
  if (newCache != null) {
    return newCache;
  }
  newCache=cache;
  if (!caches.containsKey(name)) {
    caches.put(name,newCache);
  }
  return newCache;
}
