{
  User returnvalue=null;
  for (  UserProvider provider : userproviders) {
    if (provider.isReadOnly()) {
      continue;
    }
    returnvalue=provider.createUser(username,password,name,email);
    if (returnvalue != null) {
      break;
    }
  }
  if (returnvalue == null) {
    throw new UnsupportedOperationException();
  }
  return returnvalue;
}
