{
  String domain="www.mycompany.com";
  assertTrue("Domain should be valid",StringUtils.isValidDomainName(domain));
  domain="www.my-company.com";
  assertTrue("Domain should be valid",StringUtils.isValidDomainName(domain));
  domain="abc.de";
  assertTrue("Domain should be valid",StringUtils.isValidDomainName(domain));
}
