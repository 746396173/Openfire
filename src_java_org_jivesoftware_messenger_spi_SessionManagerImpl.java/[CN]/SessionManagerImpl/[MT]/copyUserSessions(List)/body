{
  sessionLock.readLock().lock();
  try {
    Iterator users=getSessionUsers();
    while (users.hasNext()) {
      Iterator sessionItr=getSessions((String)users.next());
      while (sessionItr.hasNext()) {
        sessions.add(sessionItr.next());
      }
    }
  }
  finally {
    sessionLock.readLock().unlock();
  }
}
