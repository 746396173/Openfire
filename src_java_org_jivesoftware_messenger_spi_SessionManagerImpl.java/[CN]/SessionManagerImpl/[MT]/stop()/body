{
  sendServerMessage(null,LocaleUtils.getLocalizedString("admin.shutdown.now"));
  super.stop();
  try {
    Iterator sIter=this.getSessions();
    while (sIter.hasNext()) {
      Session session=(Session)sIter.next();
      try {
        session.getConnection().close();
      }
 catch (      Throwable t) {
      }
    }
  }
 catch (  Exception e) {
  }
}
