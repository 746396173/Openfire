{
  while (!isDone()) {
    Packet packet=packetQueue.poll();
    if (packet != null) {
      try {
        sendPacket(packet);
      }
 catch (      Exception e) {
        returnErrorToSender(packet);
        Log.debug("OutgoingSessionPromise: Error sending packet to remote server: " + packet,e);
      }
    }
  }
  promise.processorDone(this);
}
