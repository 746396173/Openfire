{
  final SSLEngine sslEngine=createSSLEngine();
  sslEngine.setUseClientMode(false);
switch (getClientAuth()) {
case needed:
    sslEngine.setNeedClientAuth(true);
  break;
case wanted:
sslEngine.setWantClientAuth(true);
break;
case disabled:
sslEngine.setWantClientAuth(false);
break;
}
return sslEngine;
}
