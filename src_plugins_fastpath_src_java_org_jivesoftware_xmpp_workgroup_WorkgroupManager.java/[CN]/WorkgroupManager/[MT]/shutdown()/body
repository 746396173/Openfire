{
  workgroups.clear();
  GroupEventDispatcher.removeListener(groupEventListener);
  instance=null;
  ChatSearchManager.shutdown();
  ChatSettingsManager.shutdown();
  RoutingManager.shutdown();
  WorkgroupProviderManager.shutdown();
