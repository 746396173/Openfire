{
  if (UserManager.getUserProvider().isReadOnly()) {
    return false;
  }
  try {
    UserManager.getInstance().createUser("demo","demo","Fastpath Demo Account","demo@fastpath.com");
    return true;
  }
 catch (  Exception e) {
    ComponentManagerFactory.getComponentManager().getLog().error(e);
  }
  return false;
}
