{
  List<ConnectionMultiplexerSession> sessions=connnectionManagerSessions.get(domain);
  if (sessions == null || sessions.isEmpty()) {
    return null;
  }
  try {
    return sessions.get(0).getConnection().getInetAddress();
  }
 catch (  UnknownHostException e) {
    Log.error(e);
    return null;
  }
}
