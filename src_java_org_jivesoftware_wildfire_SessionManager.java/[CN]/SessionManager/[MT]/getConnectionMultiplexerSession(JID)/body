{
  List<ConnectionMultiplexerSession> sessions=connnectionManagerSessions.get(address.getDomain());
  if (sessions == null || sessions.isEmpty()) {
    return null;
  }
  if (address.getResource() != null) {
    for (    ConnectionMultiplexerSession session : sessions) {
      if (session.getAddress().equals(address)) {
        return session;
      }
    }
    return null;
  }
 else {
    return sessions.get(0);
  }
}
