{
  if (username == null || !userManager.isRegisteredUser(username)) {
    return 0;
  }
  int sessionCount=0;
  SessionMap sessionMap=sessions.get(username);
  if (sessionMap != null) {
    for (    ClientSession session : sessionMap.getSessions()) {
      if (session.getPresence().isAvailable()) {
        sessionCount++;
      }
    }
  }
  return sessionCount;
}
