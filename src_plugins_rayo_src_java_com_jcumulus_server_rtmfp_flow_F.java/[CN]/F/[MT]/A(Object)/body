{
  if (obj == null)   A();
 else   if (obj instanceof E)   A((E)obj);
 else   if (obj instanceof Integer)   A(((Integer)obj).intValue());
 else   if (obj instanceof String)   A((String)obj);
 else   if (obj instanceof Number)   A(((Number)obj).doubleValue());
 else   if (obj instanceof B)   A((B)obj);
 else   A(obj.toString());
}
