{
  final int prime=31;
  int result=1;
  result=prime * result + ((nick == null) ? 0 : nick.hashCode());
  result=prime * result + ((rJID == null) ? 0 : rJID.hashCode());
  result=prime * result + ((room == null) ? 0 : room.hashCode());
  result=prime * result + ((user == null) ? 0 : user.hashCode());
  return result;
}
