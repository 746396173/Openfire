{
  if (enabled) {
    if (isClusteringEnabled() && isClusteringStarted()) {
      return;
    }
  }
 else {
    if (!isClusteringEnabled()) {
      return;
    }
  }
  JiveGlobals.setXMLProperty(CLUSTER_PROPERTY_NAME,Boolean.toString(enabled));
  if (!enabled) {
    shutdown();
  }
 else {
    JiveProperties.getInstance().init();
    startup();
  }
}
