{
  final Attribute attr=(Attribute)element.attribute("space");
  boolean preserveFound=preserve;
  if (attr != null) {
    if ("xml".equals(attr.getNamespacePrefix()) && "preserve".equals(attr.getText())) {
      preserveFound=true;
    }
 else {
      preserveFound=false;
    }
  }
  return preserveFound;
}
