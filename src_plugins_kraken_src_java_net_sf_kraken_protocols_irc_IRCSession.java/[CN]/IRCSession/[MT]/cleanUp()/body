{
  if (timer != null) {
    try {
      timer.cancel();
    }
 catch (    Exception e) {
    }
    timer=null;
  }
  if (statusCheck != null) {
    try {
      statusCheck.cancel();
    }
 catch (    Exception e) {
    }
    statusCheck=null;
  }
  if (listener != null) {
    try {
      listener.disable();
    }
 catch (    Exception e) {
    }
    listener=null;
  }
  if (autoResponder != null) {
    try {
      autoResponder.disable();
    }
 catch (    Exception e) {
    }
    autoResponder=null;
  }
  if (autoRegister != null) {
    try {
      autoRegister.disable();
    }
 catch (    Exception e) {
    }
    autoRegister=null;
  }
  if (autoReconnect != null) {
    try {
      autoReconnect.disable();
    }
 catch (    Exception e) {
    }
    autoReconnect=null;
  }
  if (connection != null) {
    try {
      connection.disconnect();
    }
 catch (    Exception e) {
    }
    connection=null;
  }
}
