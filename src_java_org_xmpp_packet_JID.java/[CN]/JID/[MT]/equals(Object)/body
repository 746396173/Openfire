{
  if (!(object instanceof JID)) {
    return false;
  }
  if (this == object) {
    return true;
  }
  JID jid=(JID)object;
  if (node != null) {
    if (!node.equals(jid.node)) {
      return false;
    }
  }
 else   if (jid.node != null) {
    return false;
  }
  if (!domain.equals(jid.domain)) {
    return false;
  }
  if (resource != null) {
    if (!resource.equals(jid.resource)) {
      return false;
    }
  }
 else   if (jid.resource != null) {
    return false;
  }
  return true;
}
