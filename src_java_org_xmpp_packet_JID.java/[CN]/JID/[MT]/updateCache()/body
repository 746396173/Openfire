{
  StringBuilder buf=new StringBuilder(40);
  if (node != null) {
    buf.append(node).append("@");
  }
  buf.append(domain);
  cachedBareJID=buf.toString();
  if (resource != null) {
    buf.append("/").append(resource);
    cachedFullJID=buf.toString();
  }
 else {
    cachedFullJID=cachedBareJID;
  }
}
