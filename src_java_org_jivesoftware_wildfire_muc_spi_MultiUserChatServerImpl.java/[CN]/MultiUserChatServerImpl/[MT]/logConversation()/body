{
  ConversationLogEntry entry=null;
  boolean success=false;
  for (int index=0; index <= log_batch_size && !logQueue.isEmpty(); index++) {
    entry=logQueue.poll();
    if (entry != null) {
      success=MUCPersistenceManager.saveConversationLogEntry(entry);
      if (!success) {
        logQueue.add(entry);
      }
    }
  }
}
