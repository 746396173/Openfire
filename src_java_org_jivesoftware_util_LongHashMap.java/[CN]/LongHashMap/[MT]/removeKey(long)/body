{
  int i=indexOfKey(key);
  if (i < 0)   return false;
  this.state[i]=REMOVED;
  this.values[i]=null;
  this.distinct--;
  if (this.distinct < this.lowWaterMark) {
    int newCapacity=chooseShrinkCapacity(this.distinct,this.minLoadFactor,this.maxLoadFactor);
    rehash(newCapacity);
  }
  return true;
}
