{
  try {
    pstmt=con.prepareStatement(CREATE_ROSTER_ITEM_GROUPS);
    pstmt.setLong(1,rosterID);
    for (int i=0; iter.hasNext(); i++) {
      pstmt.setInt(2,i);
      pstmt.setString(3,(String)iter.next());
      try {
        pstmt.executeUpdate();
      }
 catch (      SQLException e) {
        Log.error(e);
      }
    }
  }
  finally {
    try {
      if (pstmt != null) {
        pstmt.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
  }
}
