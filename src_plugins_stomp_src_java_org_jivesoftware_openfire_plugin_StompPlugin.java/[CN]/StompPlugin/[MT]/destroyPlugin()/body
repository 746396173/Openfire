{
  try {
    for (    XMPPServlet.XMPPWebSocket socket : sockets.values()) {
      try {
        LocalClientSession session=socket.getSession();
        session.close();
        SessionManager.getInstance().removeSession(session);
        session=null;
      }
 catch (      Exception e) {
      }
    }
    sockets.clear();
  }
 catch (  Exception e) {
    Log.error(" destroyPlugin exception ",e);
  }
}
