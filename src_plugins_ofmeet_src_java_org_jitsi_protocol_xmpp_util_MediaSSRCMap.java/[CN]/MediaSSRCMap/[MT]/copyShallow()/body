{
  Map<String,List<SourcePacketExtension>> mapCopy=new HashMap<String,List<SourcePacketExtension>>();
  for (  String media : ssrcs.keySet()) {
    List<SourcePacketExtension> listCopy=new ArrayList<SourcePacketExtension>(ssrcs.get(media));
    mapCopy.put(media,listCopy);
  }
  return new MediaSSRCMap(mapCopy);
}
