{
  try {
    String from=iq.getFrom();
    if ((from == null) || (from.length() == 0)) {
      JID fromJID=getJID();
      if (fromJID != null)       iq.setFrom(fromJID.toString());
    }
    Packet packet=IQUtils.convert(iq);
    send(packet);
    logd("SENT: " + packet.toXML());
  }
 catch (  Exception e) {
    loge(e);
    throw e;
  }
}
