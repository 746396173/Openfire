{
  org.jivesoftware.smack.packet.IQ response;
  if (request instanceof ColibriConferenceIQ)   response=handleColibriConferenceIQ((ColibriConferenceIQ)request);
 else   if (request instanceof GracefulShutdownIQ)   response=handleGracefulShutdownIQ((GracefulShutdownIQ)request);
 else   response=null;
  return response;
}
