{
  if (isClosed()) {
    return false;
  }
  try {
synchronized (writer) {
      SocketSendingTracker.getInstance().socketStartedSending(socket);
      writer.write(" ");
      writer.flush();
    }
  }
 catch (  Exception e) {
    Log.warn("Closing no longer valid connection" + "\n" + this.toString(),e);
    close();
  }
 finally {
    SocketSendingTracker.getInstance().socketFinishedSending(socket);
  }
  return !isClosed();
}
