{
  Log.debug("IRC privmsg: " + chan + ", "+ ircUser+ ", "+ msg);
  if (silenced) {
    return;
  }
  if (msg.equals("VERSION")) {
    return;
  }
  getSession().getTransport().sendMessage(getSession().getJIDWithHighestPriority(),getSession().getTransport().convertIDToJID(ircUser.getNick()),msg);
}
