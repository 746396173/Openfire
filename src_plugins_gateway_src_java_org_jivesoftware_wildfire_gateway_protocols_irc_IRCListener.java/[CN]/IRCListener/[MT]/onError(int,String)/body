{
  Log.debug("IRC error: " + i + ", "+ string);
  if (silenced) {
    return;
  }
  getSession().getTransport().sendMessage(getSession().getJID(),getSession().getTransport().getJID(),LocaleUtils.getLocalizedString("gateway.irc.errorreceivedwithcode","gateway",Arrays.asList(Integer.toString(i))) + " " + string,Message.Type.error);
}
