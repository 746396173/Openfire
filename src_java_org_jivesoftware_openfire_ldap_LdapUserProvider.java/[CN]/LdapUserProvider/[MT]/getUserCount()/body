{
  if (userCount != -1 && System.currentTimeMillis() < expiresStamp) {
    return userCount;
  }
  List<String> users=manager.retrieveList(manager.getUsernameField(),MessageFormat.format(manager.getSearchFilter(),"*"),-1,-1,null);
  this.userCount=users.size();
  this.expiresStamp=System.currentTimeMillis() + JiveConstants.MINUTE * 5;
  return this.userCount;
}
