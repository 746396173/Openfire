{
  Object[] keys=map.keySet().toArray();
  for (int i=0; i < keys.length; i++) {
    remove(keys[i]);
  }
  map.clear();
  lastAccessedList.clear();
  lastAccessedList=new org.jivesoftware.util.LinkedList<K>();
  ageList.clear();
  ageList=new org.jivesoftware.util.LinkedList<K>();
  cacheSize=0;
  cacheHits=0;
  cacheMisses=0;
}
