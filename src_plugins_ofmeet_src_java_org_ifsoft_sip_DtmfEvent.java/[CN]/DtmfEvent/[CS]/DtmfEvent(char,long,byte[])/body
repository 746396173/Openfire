{
  if (dtmf == '*') {
    event=10;
  }
 else   if (dtmf == '#') {
    event=11;
  }
 else   if (dtmf == 'A') {
    event=12;
  }
 else   if (dtmf == 'B') {
    event=13;
  }
 else   if (dtmf == 'C') {
    event=14;
  }
 else   if (dtmf == 'D') {
    event=15;
  }
 else {
    event=Byte.parseByte("" + dtmf);
  }
  RtpUtil.buildRtpHeader(rtpPacket,101,(short)0,startTime,ssrc);
  rtpPacket[12]=this.event;
  rtpPacket[13]=10;
  duration=0;
}
