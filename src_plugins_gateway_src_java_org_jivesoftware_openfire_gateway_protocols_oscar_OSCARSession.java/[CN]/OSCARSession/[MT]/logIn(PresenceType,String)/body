{
  if (!isLoggedIn()) {
    setLoginStatus(TransportLoginStatus.LOGGING_IN);
    loginConn=new LoginConnection(new ConnDescriptor(JiveGlobals.getProperty(propertyPrefix + ".connecthost","login.oscar.aol.com"),JiveGlobals.getIntProperty(propertyPrefix + ".connectport",5190)),this);
    loginConn.connect();
    this.presenceType=presenceType;
    this.verboseStatus=verboseStatus;
  }
 else {
    Log.warn(this.jid + " is already logged in");
  }
}
