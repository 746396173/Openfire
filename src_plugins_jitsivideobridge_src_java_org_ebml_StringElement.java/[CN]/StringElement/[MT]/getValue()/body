{
  try {
    if (data == null)     throw new java.lang.IllegalStateException("Call readData() before trying to extract the string value.");
    return new String(data,charset);
  }
 catch (  java.io.UnsupportedEncodingException ex) {
    if (checkForCharsetHack()) {
      return getValue();
    }
    ex.printStackTrace();
    return "";
  }
}
