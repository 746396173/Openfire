{
  super(registration,jid,transport,priority);
  Log.debug("Creating " + getTransport().getType() + " session for "+ registration.getUsername());
  config=new ConnectionConfiguration(JiveGlobals.getProperty("plugin.gateway." + getTransport().getType() + ".connecthost",(getTransport().getType().equals(TransportType.gtalk) ? "talk.google.com" : "jabber.org")),JiveGlobals.getIntProperty("plugin.gateway." + getTransport().getType() + ".connectport",5222),new JID(registration.getUsername()).getDomain());
  listener=new XMPPListener(this);
}
