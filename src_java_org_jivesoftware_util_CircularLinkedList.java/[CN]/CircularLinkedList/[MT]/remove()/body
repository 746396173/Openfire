{
  if (current != null) {
    currentLock.writeLock().lock();
    try {
      LinkedListNode node=current.next;
      if (node != null) {
        node.previous.next=node.next;
        node.next.previous=node.previous;
        if (node == mark) {
          mark=node.next;
        }
      }
 else {
        current=null;
      }
      size--;
    }
  finally {
      currentLock.writeLock().unlock();
    }
  }
}
