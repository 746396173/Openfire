{
  GroupEvent event=new GroupEvent(GroupEvent.EventType.group_deleting,group,Collections.EMPTY_MAP);
  GroupEventDispatcher.dispatchEvent(event);
  provider.deleteGroup(group.getName());
  groupCache.remove(group.getName());
  RosterManager rosterManager=XMPPServer.getInstance().getRosterManager();
  rosterManager.groupDeleted(group);
}
