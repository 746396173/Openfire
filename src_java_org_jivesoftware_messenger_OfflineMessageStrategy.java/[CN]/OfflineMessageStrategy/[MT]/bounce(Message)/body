{
  if (message.getFrom() == null) {
    return;
  }
  try {
    Message errorResponse=message.createCopy();
    errorResponse.setError(new PacketError(PacketError.Condition.item_not_found,PacketError.Type.continue_processing));
    errorResponse.setFrom(message.getTo());
    errorResponse.setTo(message.getFrom());
    router.route(errorResponse);
  }
 catch (  Exception e) {
    Log.error(e);
  }
}
