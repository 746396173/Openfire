{
  try {
    Session session=sessionManager.getSession(message.getFrom());
    Message errorResponse=message.createCopy();
    errorResponse.setError(new PacketError(PacketError.Condition.item_not_found,PacketError.Type.continue_processing));
    session.getConnection().deliver(errorResponse);
  }
 catch (  Exception e) {
    Log.error(e);
  }
}
