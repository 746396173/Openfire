{
  Log.info("FocusAgent - closeColibri ");
  count=0;
  focusId=null;
  if (recorder != null) {
    try {
      recorder.done();
      outDataSource.disconnect();
      outDataSource.stop();
      for (      CallSession callSession : callSessions.values()) {
        callSession.mediaStream.stop();
        callSession.mediaStream.close();
      }
      callSessions.clear();
    }
 catch (    Exception e) {
    }
    recorder=null;
    outDataSource=null;
  }
}
