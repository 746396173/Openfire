{
  List elements=element.elements();
  if (elements.isEmpty()) {
    return null;
  }
  for (int i=0; i < elements.size(); i++) {
    Element childElement=(Element)elements.get(i);
    String childNamespace=childElement.getNamespaceURI();
    if (namespace.equals(childNamespace)) {
      return childElement;
    }
  }
  return null;
}
