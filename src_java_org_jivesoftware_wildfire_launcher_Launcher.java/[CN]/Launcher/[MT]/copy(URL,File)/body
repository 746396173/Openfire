{
  InputStream in=null;
  OutputStream out=null;
  try {
    in=src.openStream();
    out=new FileOutputStream(dst);
    dst.mkdirs();
    copy(in,out);
  }
  finally {
    try {
      if (in != null) {
        in.close();
      }
    }
 catch (    IOException e) {
    }
    try {
      if (out != null) {
        out.close();
      }
    }
 catch (    IOException e) {
    }
  }
}
