  if (muteIQHandler != null) {
    server.getIQRouter().removeHandler(muteIQHandler);
    muteIQHandler=null;
  }
  if (unmuteIQHandler != null) {
    server.getIQRouter().removeHandler(unmuteIQHandler);
    unmuteIQHandler=null;
  }
  if (holdIQHandler != null) {
