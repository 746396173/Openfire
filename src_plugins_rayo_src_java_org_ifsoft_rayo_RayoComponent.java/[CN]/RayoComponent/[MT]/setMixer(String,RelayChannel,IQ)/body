{
  ConferenceManager conferenceManager=ConferenceManager.getConference(mixer,channel.getMediaPreference(),channel.getFrom().getNode(),false);
  try {
    if (conferenceManager.getMemberList().size() == 0) {
      conferenceManager.recordConference(true,mixer + "-" + System.currentTimeMillis()+ ".au","au");
    }
  }
 catch (  ParseException e1) {
    reply.setError(PacketError.Condition.internal_server_error);
  }
}
