{
  String nodeId=event.getNodeId();
  if (logger.isDebugEnabled())   logger.debug("PubSub update for node: " + nodeId);
  SubscriptionListener listener=listenerMap.get(nodeId);
  if (listener != null) {
    for (    Object item : event.getItems()) {
      if (!(item instanceof PayloadItem))       continue;
      PayloadItem payloadItem=(PayloadItem)item;
      listener.onSubscriptionUpdate(nodeId,payloadItem.getPayload());
    }
  }
}
