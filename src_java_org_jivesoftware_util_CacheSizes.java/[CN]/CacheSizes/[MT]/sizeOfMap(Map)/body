{
  if (map == null) {
    return 0;
  }
  int size=36;
  Object[] values=map.values().toArray();
  for (int i=0; i < values.length; i++) {
    size+=sizeOfString((String)values[i]);
  }
  Object[] keys=map.keySet().toArray();
  for (int i=0; i < keys.length; i++) {
    size+=sizeOfString((String)keys[i]);
  }
  return size;
}
