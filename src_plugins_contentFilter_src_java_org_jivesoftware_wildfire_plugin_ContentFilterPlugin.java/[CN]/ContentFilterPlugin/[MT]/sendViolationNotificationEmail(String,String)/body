{
  try {
    User user=UserManager.getInstance().getUser(violationContact);
    EmailService.getInstance().sendMessage(user.getName(),user.getEmail(),"Wildfire","no_reply@" + violationNotificationFrom.getDomain(),subject,body,null);
  }
 catch (  Throwable e) {
    Log.error("Content Filter: Failed to send email, please review Wildfire setup",e);
  }
}
