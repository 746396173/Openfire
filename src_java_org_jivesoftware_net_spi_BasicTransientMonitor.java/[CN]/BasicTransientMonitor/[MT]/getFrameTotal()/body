{
  long quantity=0;
  if (frameList.size() > 0) {
    frameLock.writeLock().lock();
    try {
      frameList.mark();
      while (frameList.getPassCount() == 0) {
        Sample sample=(Sample)frameList.next();
        quantity+=sample.getQuantity();
      }
    }
  finally {
      frameLock.writeLock().lock();
    }
  }
  return quantity;
}
