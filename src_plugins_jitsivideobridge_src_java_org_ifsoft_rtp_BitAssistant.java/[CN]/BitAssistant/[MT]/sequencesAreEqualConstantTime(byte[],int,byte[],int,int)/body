{
  if (array1 == null && array2 == null)   return true;
  if (array1 == null || array2 == null)   return false;
  if (array1.length < offset1 + length || array2.length < offset2 + length)   return false;
  boolean areEqual=true;
  for (int i=0; i < length; i++)   if (array1[offset1 + i] != array2[offset2 + i])   areEqual=false;
  return areEqual;
}
