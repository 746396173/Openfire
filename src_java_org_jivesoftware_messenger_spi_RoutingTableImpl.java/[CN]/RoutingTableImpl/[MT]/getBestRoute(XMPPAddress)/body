{
  ChannelHandler route=null;
  try {
    route=getRoute(node);
  }
 catch (  NoSuchRouteException e) {
    XMPPAddress defaultNode=new XMPPAddress(node.getName(),node.getHost(),"");
    route=getRoute(defaultNode);
  }
  if (route == null) {
    throw new NoSuchRouteException();
  }
  return route;
}
