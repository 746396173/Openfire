{
  int count=0;
  Connection conn=null;
  PreparedStatement pstmt=null;
  try {
    conn=DriverManager.getConnection(jdbcConnString);
    pstmt=conn.prepareStatement(groupCountSQL);
    ResultSet rs=pstmt.executeQuery();
    if (rs.next()) {
      count=rs.getInt(1);
    }
    rs.close();
  }
 catch (  SQLException e) {
    Log.error(e);
  }
 finally {
    try {
      if (pstmt != null) {
        pstmt.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
    try {
      if (conn != null) {
        conn.close();
      }
    }
 catch (    Exception e) {
      Log.error(e);
    }
  }
  return count;
}
