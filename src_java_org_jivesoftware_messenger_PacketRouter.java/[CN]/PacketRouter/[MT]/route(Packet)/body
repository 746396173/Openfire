{
  if (hasRouted(packet)) {
    return;
  }
  if (packet instanceof Message) {
    route((Message)packet);
  }
 else   if (packet instanceof Presence) {
    route((Presence)packet);
  }
 else   if (packet instanceof IQ) {
    route((IQ)packet);
  }
 else {
    throw new IllegalArgumentException();
  }
}
