{
  final Offer offer=new Offer(request,queue,getAgentRejectionTimeout());
  offer.setTimeout(info.getOfferTimeout());
  offers.add(offer);
  Thread offerThread=new Thread("Dispatch offer - queue: " + queue.getName()){
    public void run(){
      dispatch(offer);
      offers.remove(offer);
    }
  }
;
  offerThread.start();
}
