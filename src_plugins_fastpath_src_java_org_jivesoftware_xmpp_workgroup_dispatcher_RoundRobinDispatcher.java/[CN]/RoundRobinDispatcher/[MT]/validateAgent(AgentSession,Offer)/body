{
  if (agentSelector.validateAgent(session,offer)) {
    logger.debug("RR - Agent: " + session.getJID() + " MAY receive offer for request: "+ offer.getRequest());
    return true;
  }
  logger.debug("RR - Agent: " + session.getJID() + " MAY NOT receive offer for request: "+ offer.getRequest());
  return false;
}
