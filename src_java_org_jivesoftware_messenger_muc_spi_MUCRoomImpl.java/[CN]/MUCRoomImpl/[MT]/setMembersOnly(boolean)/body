{
  List<Presence> presences=new ArrayList<Presence>();
  if (membersOnly && !this.membersOnly) {
    for (    MUCRole occupant : occupants.values()) {
      if (occupant.getAffiliation().compareTo(MUCRole.Affiliation.member) > 0) {
        try {
          presences.add(kickOccupant(occupant.getRoleAddress(),null,LocaleUtils.getLocalizedString("muc.roomIsNowMembersOnly")));
        }
 catch (        NotAllowedException e) {
          Log.error(e);
        }
      }
    }
  }
  this.membersOnly=membersOnly;
  return presences;
}
