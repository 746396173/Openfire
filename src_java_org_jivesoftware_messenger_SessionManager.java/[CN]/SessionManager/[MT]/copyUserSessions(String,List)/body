{
  sessionLock.readLock().lock();
  try {
    SessionMap sessionMap=sessions.get(username);
    if (sessionMap != null) {
      Iterator sessionItr=sessionMap.getSessions();
      while (sessionItr.hasNext()) {
        sessionList.add(sessionItr.next());
      }
    }
  }
  finally {
    sessionLock.readLock().unlock();
  }
}
