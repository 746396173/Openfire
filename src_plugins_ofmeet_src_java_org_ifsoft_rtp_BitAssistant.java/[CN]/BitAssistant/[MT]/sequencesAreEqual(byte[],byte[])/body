{
  if (array1 == null && array2 == null)   return Boolean.valueOf(true);
  if (array1 == null || array2 == null)   return Boolean.valueOf(false);
  if (array1.length != array2.length)   return Boolean.valueOf(false);
  for (int i=0; i < array1.length; i++)   if (array1[i] != array2[i])   return Boolean.valueOf(false);
  return Boolean.valueOf(true);
}
