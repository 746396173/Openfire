{
  elementName=(!sName.equals("")) ? sName : qName;
  if (elementName.equals("presence")) {
    isPresence=false;
  }
 else   if (elementName.equals("basic")) {
    isStatus=false;
  }
 else   if (elementName.equals("rpid:user-input")) {
    isStatusType=false;
  }
 else   if (elementName.equals("dm:note")) {
    isStatusName=false;
  }
}
