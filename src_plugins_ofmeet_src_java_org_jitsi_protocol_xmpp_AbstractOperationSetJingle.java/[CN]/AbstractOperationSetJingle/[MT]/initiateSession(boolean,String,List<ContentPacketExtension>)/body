{
  logger.info("INVITE PEER: " + address);
  String sid=JingleIQ.generateSID();
  JingleSession session=new JingleSession(sid,address);
  sessions.put(sid,session);
  JingleIQ inviteIQ=JinglePacketFactory.createSessionInitiate(getOurJID(),address,sid,contents);
  if (useBundle) {
    GroupPacketExtension group=GroupPacketExtension.createBundleGroup(contents);
    inviteIQ.addExtension(group);
    for (    ContentPacketExtension content : contents) {
      content.addChildExtension(new BundlePacketExtension());
    }
  }
  getConnection().sendPacket(inviteIQ);
}
