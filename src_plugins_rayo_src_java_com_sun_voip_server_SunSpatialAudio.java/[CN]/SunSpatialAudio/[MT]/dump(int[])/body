{
  System.out.println("\nleft " + c.length);
  for (int i=0; i < c.length; i+=2) {
    System.out.print(Integer.toHexString(c[i] & 0xff) + " ");
  }
  System.out.println("\n\nright " + c.length);
  for (int i=1; i < c.length; i+=2) {
    System.out.print(Integer.toHexString(c[i] & 0xff) + " ");
  }
  System.out.println("\n");
}
