{
  out.append(ROOM_ATTR_NAME).append("=").append("'").append(room).append("' ");
  if (ready != null) {
    out.append(READY_ATTR_NAME).append("=").append("'").append(ready).append("' ");
  }
  if (!StringUtils.isNullOrEmpty(focusJid)) {
    out.append(FOCUS_JID_ATTR_NAME).append("=").append("'").append(focusJid).append("' ");
  }
}
