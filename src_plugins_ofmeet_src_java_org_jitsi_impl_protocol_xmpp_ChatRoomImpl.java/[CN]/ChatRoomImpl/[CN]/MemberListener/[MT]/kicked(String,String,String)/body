{
  if (logger.isTraceEnabled())   logger.trace("Kicked: " + participant + ", "+ s2+ ", "+ s3);
  ChatMemberImpl member;
synchronized (members) {
    member=removeMember(participant);
  }
  if (member == null) {
    logger.error("Kicked participant does not exist: " + participant);
    return;
  }
  notifyParticipantKicked(member);
}
