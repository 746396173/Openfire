{
  if (participant.endsWith("/" + myNickName)) {
    resetCachedUserRole();
  }
 else {
    ChatMemberImpl member=members.get(participant);
    if (member != null) {
      member.resetCachedRole();
    }
 else {
      logger.error("Role reset for: " + participant + " who does not exist");
    }
  }
}
