{
  if (MUCRole.OWNER == affiliation) {
    return "owner";
  }
 else   if (MUCRole.ADMINISTRATOR == affiliation) {
    return "admin";
  }
 else   if (MUCRole.MEMBER == affiliation) {
    return "member";
  }
 else   if (MUCRole.OUTCAST == affiliation) {
    return "outcast";
  }
  return "none";
}
