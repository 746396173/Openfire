{
  MediaProxy media=new MediaProxy();
  ProxyCandidate ag=media.addSmartAgent("abc","tester","200.233.136.54",20001,"200.233.136.54",20008);
  ag.start();
  try {
    Thread.sleep(5000);
    System.out.println(InetAddress.getByName("0.0.0.0"));
  }
 catch (  Exception e) {
  }
  ag.stopAgent();
  System.out.println("Closed");
}
