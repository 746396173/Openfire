{
  PropertyEventDispatcher.removeListener(this);
  if (componentManager != null) {
    try {
      componentManager.removeComponent(serviceName);
    }
 catch (    Exception e) {
      Log.error(e);
    }
  }
  componentManager=null;
  pluginManager=null;
  sessionManager=null;
  groupManager=null;
  allowedUsers.clear();
}
