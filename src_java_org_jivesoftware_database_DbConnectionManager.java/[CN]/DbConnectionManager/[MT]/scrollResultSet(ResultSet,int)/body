{
  if (isScrollResultsSupported()) {
    if (rowNumber > 0) {
      rs.setFetchDirection(ResultSet.FETCH_FORWARD);
      try {
        rs.relative(rowNumber);
      }
 catch (      SQLException e) {
        for (int i=0; i < rowNumber; i++) {
          rs.next();
        }
      }
    }
  }
 else {
    for (int i=0; i < rowNumber; i++) {
      rs.next();
    }
  }
}
