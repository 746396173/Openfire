{
  try {
    if (keystore != null) {
      keyStore.store(new FileOutputStream(keystore),keypass.toCharArray());
    }
    if (truststore != null) {
      trustStore.store(new FileOutputStream(truststore),trustpass.toCharArray());
    }
  }
 catch (  IOException e) {
    throw e;
  }
catch (  Exception e) {
    throw new IOException(e.getMessage());
  }
}
