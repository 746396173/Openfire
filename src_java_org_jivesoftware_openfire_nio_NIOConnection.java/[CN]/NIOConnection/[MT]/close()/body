{
  boolean closedSuccessfully=false;
synchronized (this) {
    if (!isClosed()) {
      if (session != null) {
        session.setStatus(Session.STATUS_CLOSED);
      }
      try {
        deliverRawText(flashClient ? "</flash:stream>" : "</stream:stream>",false);
      }
 catch (      Exception e) {
      }
      ioSession.close();
      closed=true;
      closedSuccessfully=true;
    }
  }
  if (closedSuccessfully) {
    notifyCloseListeners();
  }
}
