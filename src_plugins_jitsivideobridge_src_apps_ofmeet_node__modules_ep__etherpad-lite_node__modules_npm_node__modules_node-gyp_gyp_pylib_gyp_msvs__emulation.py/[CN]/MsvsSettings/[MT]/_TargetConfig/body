def _TargetConfig(self, config):
    'Returns the target-specific configuration.'
    arch = self.GetArch(config)
    if ((arch == 'x64') and (not config.endswith('_x64'))):
        config += '_x64'
    if ((arch == 'x86') and config.endswith('_x64')):
        config = config.rsplit('_', 1)[0]
    return config
