{
  if (id == null) {
    return null;
  }
  Item child=getItem(id);
  Item root=null;
  if (child != null) {
    Item parent=child.getParent();
    root=parent;
    while (parent != null) {
      parent=parent.getParent();
      if (parent != null) {
        root=parent;
      }
    }
  }
  return root;
}
