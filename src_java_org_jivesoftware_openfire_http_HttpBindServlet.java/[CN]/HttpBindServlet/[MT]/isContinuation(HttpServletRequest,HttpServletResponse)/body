{
  HttpSession session=(HttpSession)request.getAttribute("request-session");
  if (session == null) {
    return false;
  }
synchronized (session) {
    try {
      respond(session,request,response,session.consumeResponse((HttpConnection)request.getAttribute("connection")),request.getMethod());
    }
 catch (    HttpBindException e) {
      sendError(request,response,e.getBindingError(),session);
    }
  }
  return true;
}
