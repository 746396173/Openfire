{
  if (token == null) {
    throw new NullPointerException("token");
  }
  if (userJid == null) {
    throw new NullPointerException("userJid");
  }
  if (roomName == null) {
    throw new NullPointerException("roomName");
  }
  this.token=token;
  this.userJid=userJid;
  this.roomName=roomName;
  creationTimestamp=System.currentTimeMillis();
}
